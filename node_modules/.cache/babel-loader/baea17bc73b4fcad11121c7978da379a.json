tatus: function changeBtnStatus() {\n      this.itemToAdd = {};\n      this.showInputs = !this.showInputs;\n    },\n    notifyVue: function notifyVue(verticalAlign, horizontalAlign, state) {\n      this.$notify({\n        message: '¡NUEVO ITEM AL CARRITO!',\n        icon: 'add_alert',\n        horizontalAlign: horizontalAlign,\n        verticalAlign: verticalAlign,\n        type: state\n      });\n    },\n    showSwal: function showSwal(type, totalVenta) {\n      var _this = this;\n\n      swal({\n        title: 'Venta registrada!',\n        text: '$ ' + totalVenta + ' CLP',\n        type: 'success',\n        showCancelButton: true,\n        confirmButtonClass: 'md-button md-success',\n        cancelButtonClass: 'md-button md-info',\n        confirmButtonText: 'ok',\n        cancelButtonText: 'enviar',\n        buttonsStyling: false\n      }).then(function (result) {\n        if (!result.value) {\n          swal({\n            title: 'Correo enviado!',\n            text: 'hectorluisgonzalezlarreal@gmail.com',\n            type: 'success',\n            confirmButtonClass: 'md-button md-success',\n            buttonsStyling: false\n          }).then(function () {\n            _this.$router.push('ventas');\n          });\n        } else {\n          _this.ventaObject = {};\n\n          _this.$router.push('ventas');\n        }\n      });\n    },\n    facturar_venta: function facturar_venta(venta) {\n      alert(venta.asunto);\n    }\n  }\n};",{"version":3,"sources":["income.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgSA,SAAA,IAAA,QAAA,cAAA;AACA,SAAA,QAAA,EAAA,WAAA,QAAA,cAAA;AACA,OAAA,WAAA,MAAA,2BAAA;AACA,OAAA,MAAA,MAAA,yBAAA;AACA,OAAA,IAAA,MAAA,aAAA;AACA,SAAA,KAAA,QAAA,cAAA;AAGA,eAAA;AACA,MADA,kBA